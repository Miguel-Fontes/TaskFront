{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch index.html",
			"type": "chrome",
			"request": "launch",
			"file": "index.html"
		},
		{
			"name": "Launch localhost with sourcemaps",
			"type": "chrome",
			"request": "launch",
			"url": "file:///C:/Temp/Node/index.html",
			"sourceMaps": true,
			"webRoot": "wwwroot"
		},
		{
			"name": "Attach",
			"type": "chrome",
			"request": "attach",
			"port": 5555
		}
	]
}